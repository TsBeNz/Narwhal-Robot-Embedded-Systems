#include "pid.h"

void PID_Vform(float c, float k_p, float k_i, float k_d, float setpoint, float feedback, float *prev_error, float *pprev_error, ){
	error_k = setpoint - feedback;

	y = c*setpoint + k_p
	error_prevk = error_k;
	error_pprevk = error_prevk;
}


